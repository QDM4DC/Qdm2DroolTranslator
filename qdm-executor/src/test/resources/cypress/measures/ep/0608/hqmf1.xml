      --><component typeCode="COMP">
      <section>
         <code code="57026-7" codeSystem="2.16.840.1.113883.6.1" displayName="Population criteria"/>
         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND:  FIRST: "Occurrence A of Procedure, Performed: Delivery" during "Measurement Period"</item>
                     <item>AND NOT: "Occurrence A of Diagnosis, Active: Full Term Delivery" ends before start of "Occurrence A of Procedure, Performed: Delivery"</item>
                     <item>AND: "Patient Characteristic Birthdate: birth date"  &gt;= 12 year(s) starts before start of "Measurement Period"</item>
                     <item>AND: "Patient Characteristic Sex: Female"</item>
                     <item>AND: "Occurrence A of Diagnosis, Active: Full Term Delivery" starts before or during "Occurrence A of Procedure, Performed: Delivery"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exclusions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: "Laboratory Test, Performed: HBsAg"  &lt;= 280 day(s) starts before start of "Occurrence A of Procedure, Performed: Delivery"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exceptions =</content>
                  <list>
                     <item>AND: <list>
                           <item>OR: "Diagnosis, Active: Hepatitis B"</item>
                           <item>OR: "Diagnosis, Inactive: Hepatitis B"</item>
                           <item> starts before start of "Occurrence A of Procedure, Performed: Delivery"</item>
                        </list>
                     </item>
                  </list>
               </item>
            </list>
